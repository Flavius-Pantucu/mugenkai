{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "target": "es2015", // Ensure the target is at least ES2015
    "lib": ["es2015"], // Add ES2015 or later
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["*.ts"]
}
